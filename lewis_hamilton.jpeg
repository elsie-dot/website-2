    function loadUniqueItem(className) {
        var list = [];
        $('.' + className).each(function () {
            var levelName = $(this)[0].innerHTML;
            if (list.length == 0 || list.indexOf(levelName) == -1) {
                list.push(levelName);
            }
        });
        return list;
    }

    function bindItemsToSelect(list, selectId) {
        for (var i = 0; i < list.length; i++) {
            $('#' + selectId)[0].add(new Option(list[i], list[i]));
        }
    }

    function appendTopRaceYearOptions() {
        var listCarLevel = loadUniqueItem('topRaceYear');
        bindItemsToSelect(listCarLevel, 'selectTopRaceYear')
    }

    function appendTopRaceLevelOptions() {
        var listCarLevel = loadUniqueItem('topRaceLevel');
        bindItemsToSelect(listCarLevel, 'selectTopRaceLevel')
    }

    function updateTopRecordInfoBySelectItem() {
        var raceYear = $("#selectTopRaceYear")[0].value;//Ëµõ‰∫ãÂπ¥Â∫¶
        var raceLevel = $("#selectTopRaceLevel")[0].value;//Ëµõ‰∫ãÂπ¥Â∫¶
        var raceTrack = $("#selectTopRaceTrack")[0] ? $("#selectTopRaceTrack")[0].value : "0";//ËµõËΩ¶Âú∫
        // alert(raceLevel);
        $(".topRaceItem").each(function () {
            var isNeedShow = true;
            var itemYear = $(this).find('.topRaceYear')[0].innerHTML;
            // alert(itemYear);
            if (raceYear != '0' && raceYear != itemYear) {
                isNeedShow = false;
            }
            var itemLevel = $(this).find('.topRaceLevel')[0].innerHTML;
            // alert(itemYear);
            if (raceLevel != '0' && raceLevel != itemLevel) {
                isNeedShow = false;
            }
            //raceTrackerÂèØËÉΩ‰∏çÂ≠òÂú®ÔºåÈúÄË¶Å‰∏âÂÖÉËøêÁÆóÂÖºÂÆπ‰∏Ä‰∏ã
            var itemTrack = $(this).find('.topRaceTrack')[0] ? $(this).find('.topRaceTrack')[0].innerHTML : "";
            if (raceTrack != '0' && raceTrack != itemTrack) {
                isNeedShow = false;
            }
            if (isNeedShow) {
                $(this).show();
            } else {
                $(this).hide();
            }
        });
    }

    function appendTopRaceTrackOptions() {
        var listCarTrack = loadUniqueItem('topRaceTrack');
        bindItemsToSelect(listCarTrack, 'selectTopRaceTrack')
    }

    function initChampionData() {
        // alert("===>");
        appendTopRaceYearOptions();
        appendTopRaceTrackOptions();
        appendTopRaceLevelOptions();
        $("#selectTopRaceYear").change(function () {
            updateTopRecordInfoBySelectItem();
        });
        $("#selectTopRaceTrack").change(function () {
            updateTopRecordInfoBySelectItem();
        });
        $("#selectTopRaceLevel").change(function () {
            updateTopRecordInfoBySelectItem();
        });
    }

    $(function () {
        initChampionData();
    })
